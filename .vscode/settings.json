{
  "deno.enable": true,
  "deno.lint": true,
  "deno.unstable": true,
  "editor.defaultFormatter": "denoland.vscode-deno",
  "[typescript]": {
    "editor.defaultFormatter": "denoland.vscode-deno",
    "editor.formatOnSave": true
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "denoland.vscode-deno",
    "editor.formatOnSave": true
  },
  "cSpell.words": [
    "axios",
    "daisyui",
    "hookform",
    "lcov",
    "signin",
    "signup",
    "supabase",
    "tabler",
    "versionstamp",
    "slackhandle",
    "twitterhandle",
    "birthdate",
    "squircle"
  ],
  "githubPullRequests.ignoredPullRequestBranches": [
    "main"
  ]
}
